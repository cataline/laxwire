{
  "name": "laxwire",
  "version": "1.0.0",
  "repository": "git@github.com:cataline/laxwire.git",
  "author": "Anastasia Kucherova <lacataline@gmail.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "slack",
    "bot",
    "telegram",
    "integration"
  ],
  "files": [
    "dist/**/*"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext=.ts --cache --cache-location=.cache/eslint",
    "test": "jest",
    "start": "yarn node ./dist/index.js",
    "dev": "tsc-watch --onSuccess \"yarn node ./dist/index.js\""
  },
  "dependencies": {
    "@google-cloud/firestore": "^3.8.2",
    "@slack/bolt": "^2.1.1",
    "@yarnpkg/pnpify": "^2.0.0-rc.22",
    "dotenv": "^8.2.0",
    "firebase-admin": "^8.12.1",
    "pino": "^6.3.2",
    "telegraf": "^3.38.0",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/node": "^14.0.11",
    "@types/pino": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "pino-pretty": "^4.0.0",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.0",
    "tsc-watch": "^4.2.8"
  },
  "dependenciesMeta": {
    "@google-cloud/firestore": {
      "unplugged": true
    },
    "@slack/bolt": {
      "unplugged": true
    },
    "@slack/logger": {
      "unplugged": true
    },
    "@slack/types": {
      "unplugged": true
    },
    "@slack/web-api": {
      "unplugged": true
    },
    "firebase-admin": {
      "unplugged": true
    },
    "telegraf": {
      "unplugged": true
    },
    "telegram-typings": {
      "unplugged": true
    },
    "unplug": {
      "unplugged": true
    },
    "yarn": {
      "unplugged": true
    }
  }
}